GLIB_VERSION_CUR_STABLE equ G_ENCODE_VERSION(GLIB_MAJOR_VERSION, GLIB_MINOR_VERSION + 1)
else
GLIB_VERSION_CUR_STABLE equ G_ENCODE_VERSION(GLIB_MAJOR_VERSION, GLIB_MINOR_VERSION)
endif
