endif

if GLIB_VERSION_MIN_REQUIRED GT GLIB_VERSION_CUR_STABLE
.err <"GLIB_VERSION_MIN_REQUIRED must be = GLIB_VERSION_CUR_STABLE">
endif
if GLIB_VERSION_MAX_ALLOWED LT GLIB_VERSION_MIN_REQUIRED
.err <GLIB_VERSION_MAX_ALLOWED must be GE GLIB_VERSION_MIN_REQUIRED>
endif
if GLIB_VERSION_MIN_REQUIRED LT GLIB_VERSION_2_26
