.err <"Only glib.h can be included directly.">
endif

G_ENCODE_VERSION macro major,minor
%	exitm<@CatStr( %((major) SHL 16 OR (minor) SHL 8) )>
	endm

GLIB_VERSION_2_26 equ G_ENCODE_VERSION(2, 26)
GLIB_VERSION_2_28 equ G_ENCODE_VERSION(2, 28)
